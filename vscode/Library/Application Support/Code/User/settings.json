{
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.formatOnSave": true,
  "cSpell.userWords": [
    "autofocus",
    "autovalidate",
    "awscdk",
    "bools",
    "bottomsheet",
    "Catppuccin",
    "Colour",
    "combinators",
    "cupertino",
    "debuggable",
    "deeplink",
    "deeplinks",
    "demangle",
    "endtemplate",
    "Flushbar",
    "fluttertoast",
    "fvmrc",
    "intellij",
    "interps",
    "ints",
    "linkify",
    "LTRB",
    "Lucide",
    "mocktail",
    "openai",
    "opsz",
    "Parwani",
    "pincode",
    "plpgsql",
    "postalpincode",
    "Posthog",
    "Pressable",
    "psql",
    "pubspec",
    "readmore",
    "redeclares",
    "respository",
    "riverpod",
    "rxdart",
    "shadcn",
    "sqflite",
    "squircle",
    "strikethrough",
    "subarray",
    "subarrays",
    "sublist",
    "supabase",
    "tearoffs",
    "todos",
    "Tokenable",
    "unawaited",
    "usecase",
    "utsname",
    "vsync",
    "webp",
    "wght",
    "writeln",
    "xxxxl",
    "Yogesh",
  ],
  "cSpell.enabledFileTypes": {
    "*": true,
    "markdown": true,
    "json": false,
    "dotenv": false,
  },
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    "*.g.dart",
  ],
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "workbench.sideBar.location": "right",
  "editor.fontSize": 15,
  "editor.lineNumbers": "relative",
  "editor.fontFamily": "FiraCode Nerd Font Mono",
  "editor.fontLigatures": true,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "dart.lineLength": 100,
  "git.openRepositoryInParentFolders": "never",
  "material-icon-theme.files.color": "#90a4ae",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.saturation": 1,
  "material-icon-theme.folders.associations": {
    "presentation": "layout",
    "network": "connection",
    "features": "project",
    "domain": "class",
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
  },
  "workbench.colorCustomizations": {
    "[Material Theme Palenight High Contrast]": {},
  },
  "debug.inlineValues": "on",
  "go.useLanguageServer": true,
  "gopls": {
    "formatting.gofumpt": true,
  },
  "testing.openTesting": "neverOpen",
  "github.copilot.editor.enableAutoCompletions": true,
  "workbench.editor.editorActionsLocation": "titleBar",
  "markdown.extension.tableFormatter.normalizeIndentation": false,
  "markdown-mermaid.darkModeTheme": "dark",
  "go.toolsManagement.autoUpdate": true,
  "workbench.editor.showTabs": "none",
  "editor.minimap.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "editor.inlineSuggest.edits.experimental.enabled": false,
  "testing.automaticallyOpenTestResults": "neverOpen",
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.overrideCopy": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d", "d"],
      "commands": ["workbench.action.debug.start"],
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"],
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["editor.action.revealDefinition"],
    },
    {
      "before": ["<leader>", "g", "i"],
      "commands": ["editor.action.goToImplementation"],
    },
    {
      "before": ["<leader>", "c", "r"],
      "commands": ["editor.action.rename"],
    },
  ],
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "dart.checkForSdkUpdates": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "dart.closeDevTools": "ifOpened",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "window.commandCenter": false,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "html.preview.doubleClickToSwitchToEditor": false,
  "git.autofetch": true,
  "kotlin.languageServer.enabled": false,
  "kotlin.debugAdapter.enabled": false,
  "codeium.enableCodeLens": false,
  "cursor.cpp.disabledLanguages": ["plaintext", "scminput"],
  "git.blame.editorDecoration.enabled": true,
  "workbench.editor.enablePreview": false,
  "workbench.activityBar.location": "hidden",
}
